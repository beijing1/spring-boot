  <xs:simpleType name="responseidtype">
    <xs:annotation>
      <xs:documentation>Response ID<br/><br/> An index pointer used to retrieve the result of E911
        batch provisioning request.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int"/>
  </xs:simpleType>
  <xs:complexType name="pongrouptype">
    <xs:annotation>
      <xs:documentation>Pon Group<br/><br/> This contains all related elements require to uniquely
        identify an order.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CCNA" type="ccnatype">
        <xs:annotation>
          <xs:documentation>Customer Carrier Name Abbreviation </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ID" type="idtype">
        <xs:annotation>
          <xs:documentation>Identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Version" type="versiontype">
        <xs:annotation>
          <xs:documentation>Version<br/><br/> Each time a change or modification is submitted
            version has to change.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CustomerType" type="customertypetype">
        <xs:annotation>
          <xs:documentation>Customer Type<br/><br/>M - magicJack Customer<br/>H - Hotel
            Customer<br/> S - SMB Customer<br/>X - Telefonica Customer<br/>I - Connect
            Customer<br/>B - BroadSmart Customer </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="diddetailtype">
    <xs:annotation>
      <xs:documentation>DID Detail<br/><br/> This contains DID and Serial_no.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DID" type="didtype">
        <xs:annotation>
          <xs:documentation>WTN</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Serial_no" type="serialnotype">
        <xs:annotation>
          <xs:documentation>Serial No</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="responsebasetype">
    <xs:sequence>
      <xs:element name="PON" type="pongrouptype">
        <xs:annotation>
          <xs:documentation>Purchase Order Number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ResultCode" type="resultcodetype">
        <xs:annotation>
          <xs:documentation>Result Code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Result" type="resulttype">
        <xs:annotation>
          <xs:documentation>Result Message</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ResponseID" type="responseidtype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Response ID</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="didqueryresultsettype">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="DID" type="didtype"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="queryportstatusresultsettype">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="Status" type="statusxmltype"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="vanityqueryresultsettype">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="DID" type="didtype"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="resultsettype">
    <xs:sequence>
      <xs:any/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="serviceaddresstype">
    <xs:sequence>
      <xs:element name="Address1" type="address1type" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Address Line 1<br/><br/> Example: 128C East Lakeview Drive Northwest,
            Franklin TN 37067<br/><br/> In this example, "128C East Lakeview Drive" is the
            Address1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Address2" type="address2type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Address Line 2<br/><br/> Example: 128C East Lakeview Drive Northwest,
            Bld 12C, Franklin TN 37067<br/><br/> In this example "Bld 12C" is
            Address2</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SANO" type="sanotype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Address Number - aka House Number<br/><br/> Example:
            128C East Lakeview Drive Northwest, Franklin TN 37067<br/><br/> In this example the
            House Numbrer is 128 </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SASF" type="sasftype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Number Suffix - Alpa addition<br/><br/> Example: 128C
            East Lakeview Drive Northwest, Franklin TN 37067<br/><br/> In this example the Number
            Suffix is C </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SAFD" type="safdtype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Street Directional Prefix<br/><br/> Example: 128C E
            Lakeview Drive Northwest, Franklin TN 37067<br/><br/> In this example the Prefix is
            E<br/><br/> NOTE: this must be abbreviated. East=E, North East = NE, etc
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SASN" type="sasntype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Street Name<br/><br/> Example: 128C East Lakeview Drive
            Northwest, Franklin TN 37067<br/><br/> In this example the Name is Lakeview
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SASS" type="sasstype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Street Suffix<br/><br/> Example: 128C East Lakeview
            Drive Northwest, Franklin TN 37067<br/><br/> In this example the Street Suffix is
            NW<br/><br/> Must also be abbrevated </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SATH" type="sathtype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Street Type<br/><br/> Example: 128C East Lakeview Drive
            Northwest, Franklin TN 37067<br/><br/> In this example the Street Type is Dr
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SAAI" type="saaitype" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Service Address Additional Information<br/><br/> Example: 128C East
            Lakeview Drive Northwest, Apt 13C, Franklin TN 37067<br/><br/> In this example the
            Additional Information Type is Apt 13C </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="City" type="citytype">
        <xs:annotation>
          <xs:documentation>Service Address City<br/><br/> Example: 128C East Lakeview Drive
            Northwest, Franklin TN 37067<br/><br/> In this example the City is Franklin
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="State" type="statetype">
        <xs:annotation>
          <xs:documentation>Service Address State<br/><br/> Example: 128C East Lakeview Drive
            Northwest, Franklin TN 37067<br/><br/> In this example the State is TN
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Zip" type="ziptype">
        <xs:annotation>
          <xs:documentation>Service Address Zip<br/><br/> Example: 128C East Lakeview Drive
            Northwest, Franklin TN 37067<br/><br/> In this example the City is 37067
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Country" type="countrytype" minOccurs="0" default="US">
        <xs:annotation>
          <xs:documentation>Service Address Country<br/><br/> Example: 128C East Lakeview Drive
            Northwest, Franklin TN 37067 US<br/><br/> In this example the Country is US
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="requestouttype">
    <xs:annotation>
      <xs:documentation>Request Out<br/><br/> This inicate a Request from the Partner to the LNP
        Provider or the E911 Provider or the DID Provider.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="DIDList" type="didlisttype">
        <xs:annotation>
          <xs:documentation>A list of Reserve and or Activate phone numbers</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NewPort" type="newportintype">
        <xs:annotation>
          <xs:documentation>Request to Port number(s) in</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CanPort" type="canporttype">
        <xs:annotation>
          <xs:documentation>Request to Port number(s) in</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SetCNAM" type="setcnamtype">
        <xs:annotation>
          <xs:documentation>Request to Set the Caller ID on a WTN</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CancelPort" type="cancelportintype">
        <xs:annotation>
          <xs:documentation>Cancel a pending Port In Request</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ResubmitPort" type="resubmitportintype">
        <xs:annotation>
          <xs:documentation>Resubmit an existing (uncompleted) Port In request</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SetE911" type="sete911type">
        <xs:annotation>
          <xs:documentation>Submit address to be used by E911</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UpdateE911" type="updatee911type">
        <xs:annotation>
          <xs:documentation>Submit an address change to be used by E911</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RemoveE911" type="removee911type">
        <xs:annotation>
          <xs:documentation>Remove address from E911 database</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="GetE911RespCode" type="gete911provcodetype">
        <xs:annotation>
          <xs:documentation>Get provisioning response from E911 database</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="QueryDIDRange" type="querydidrangetype" form="qualified">
        <xs:annotation>
          <xs:documentation>Query and return a set of Did's</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="QueryPortStatus" type="queryportstatustype" form="qualified">
        <xs:annotation>
          <xs:documentation>Query the status for a port by TN</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="QueryVanityDIDRange" type="queryvanitydidrangetype" form="qualified">
        <xs:annotation>
          <xs:documentation>Query and return a set of Vanity Did's</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="QueryAreaCodes" type="queryareacodes"/>
      <xs:element name="QueryExchange" type="queryexchange"/>
      <xs:element name="ReserveDID" type="reservedidtype">
        <xs:annotation>
          <xs:documentation>Reserve DID's</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AssignDID">
        <xs:annotation>
          <xs:documentation>Assign DID</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="PON" type="pongrouptype">
              <xs:annotation>
                <xs:documentation>Purchase Order Number</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AN" type="antype">
              <xs:annotation>
                <xs:documentation>Account Number<br/><br/> This is the partners account number
                  associated with the WTN being assigned.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DID" type="didtype">
              <xs:annotation>
                <xs:documentation>Working Telephone Number<br/><br/> This is the telephone number
                  you wish to assign</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Serial_no" type="serialnotype">
              <xs:annotation>
                <xs:documentation>Serial Number<br/><br/> This should be a unique representation of
                  the port on the device this DID will be associated with. It should include the
                  port no. Thus maybe something like SERIAL+PORT. </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ReleaseDID">
        <xs:annotation>
          <xs:documentation>Release DID</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="PON" type="pongrouptype">
              <xs:annotation>
                <xs:documentation>Purchase Order Number</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DID" type="didtype">
              <xs:annotation>
                <xs:documentation>Working Telephone Number<br/><br/> This is the telephone number
                  you wish to release.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ReserveDaysOut" type="reservedaysouttype" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Reserve Days Out<br/><br/> Number of days in the future to set the
                  reserve date to.<br/> The default is 60 Days if no value is passed in. A value of
                  0 will set the Reserve Date to the current datetime and effectively make the DID
                  available immediatelly to be queried and reserved. </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="requestintype">
    <xs:annotation>
      <xs:documentation>Request In<br/><br/> This indicate a Request from the LNP Provider or the
        E911 Provider or the DID Provider to the Partner.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="PortOut" type="portouttype">
        <xs:annotation>
          <xs:documentation>A request from another carrier to port out a number
            out</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PortOutSup" type="portoutsuptype">
        <xs:annotation>
          <xs:documentation>A sup request from another carrier to a port out
            request</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CustomerServiceRecord">
        <xs:annotation>
          <xs:documentation>A request from another carrier for the Customer Service
            Record.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="PON" type="pongrouptype"/>
            <xs:element name="WTN" type="didtype"/>
            <xs:any maxOccurs="unbounded" minOccurs="0">
              <xs:annotation>
                <xs:documentation>NOTE: There may be an additional fields supplied by the carrier
                  requesting the Customer Service Record that he received from the customer that
                  would help us identify the correct account and ensure the request would not be
                  consider a slamming request.</xs:documentation>
              </xs:annotation>
            </xs:any>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="portouttype">
    <xs:annotation>
      <xs:documentation>Port Out Request<br/><br/> This is a request the Partner will receive from
        another carrier stating the customer desires to move this number to the sending
        carrier.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PON" type="pongrouptype">
        <xs:annotation>
          <xs:documentation>PON</xs:documentation>
        </xs:annotation>
      </xs:element>
