          <xs:documentation>Billing Telephone Number Not a required field; this should be the
            telephone number to associate the WTN with.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" type="nametype">
        <xs:annotation>
          <xs:documentation>This is the name associated with the WTN on the
            account.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="E911Address" type="serviceaddresstype">
        <xs:annotation>
          <xs:documentation>This is the E911 Address associated with the WTN on the
            account.</xs:documentation>
        </xs:annotation>
